    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.26.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "prettier": "2.8.7",
